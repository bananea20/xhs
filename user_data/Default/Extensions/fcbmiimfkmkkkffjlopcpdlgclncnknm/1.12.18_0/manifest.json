{
   "background": {
      "scripts": [ "scripts/3p/hot-reload.js", "scripts/fsXBrowserSupport.js", "scripts/fsPreferences.js", "scripts/fsUtils.js", "scripts/fsCommands.js", "scripts/fsBackground.js", "scripts/fsSSS.js", "scripts/fsNativeAPISupport.js", "scripts/fsWASMAPISupport.js", "scripts/fsAutomation.js" ]
   },
   "browser_action": {
