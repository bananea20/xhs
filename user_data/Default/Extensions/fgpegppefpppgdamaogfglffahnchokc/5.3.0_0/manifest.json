{
   "author": "晚星",
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon32.png",
      "default_popup": "index.html",
      "default_title": "Magnetar"
   },
   "content_scripts": [ {
      "js": [ "magnetar_script.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "description": "加速访问谷歌网站",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "32": "icon32.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Magnetar",
   "omnibox": {
      "keyword": "q"
   },
   "permissions": [ "*://*/*", "contextMenus", "storage", "proxy", "webRequest", "webRequestBlocking" ],
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "5.3.0"
}
